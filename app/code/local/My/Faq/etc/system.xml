<?xml version="1.0" ?>
<!--Frontend input types in lib/Varien/Data/Form/Element-->
<config>
    <tabs>
        <faq translate="label" module="my_faq">
            <label>My Faq Module</label>
            <sort_order>200</sort_order>
        </faq>
    </tabs>
    <sections>
        <faq module="my_faq" translate="label">
            <class>separator-top</class>
            <label>FAQ</label>
            <tab>faq</tab>
            <frontend_type>text</frontend_type>
            <sort_order>1</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_Website>1</show_in_Website>
            <show_in_store>1</show_in_store>
            <groups>
                <test translate="label">
                    <label>test</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>100</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <module_status translate="label">
                            <label>Is module enabled</label>
                            <frontend_type>select</frontend_type>
                            <source_model>faq/source_myoptions</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </module_status>
                        <module_text translate="label">
                            <label>Some text</label>
                            <frontend_type>textarea</frontend_type>
                            <sort_order>5</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <depends><module_status>0</module_status></depends>
                        </module_text>
                    </fields>
                </test>
            </groups>
        </faq>
    </sections>
</config>